executable := complicated

source := main.c complicated.c

object:= $(source:.c=.o)

$(executable):$(object)
	gcc -o complicated  complicated.o main.o

main.o:main.c
	gcc -o main.o -c main.c

complicated.o:complicated.c
	gcc -o complicated.o -c complicated.c


.PHONY:clean
clean:
	rm *.o
	rm complicated
